{
  "name": "fairdataihub-website",
  "description": "Homepage for the Fair Data Innovations Hub",
  "version": "2.29.0",
  "private": true,
  "author": "Fair Data Innovations Hub <info@fairdataihub.org> (https://fairdataihub.org)",
  "license": "MIT",
  "keywords": [
    "fair data",
    "fairhub",
    "fairdataihub",
    "calmi2",
    "calmii"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|jsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|jsx)\" --write",
    "prepare": "husky install",
    "postinstall": "husky install",
    "commit": "cz",
    "postbuild": "next-sitemap",
    "semantic-release": "semantic-release",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome --headless \"cypress/e2e/**/*.cy.js\""
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@giscus/react": "2.3.0",
    "@lottiefiles/lottie-player": "2.0.2",
    "@sendgrid/mail": "7.7.0",
    "cookies-next": "2.1.2",
    "dayjs": "1.11.9",
    "formik": "2.4.3",
    "gray-matter": "4.0.3",
    "markdown-it": "13.0.1",
    "next": "13.4.2",
    "plaiceholder": "^2.5.0",
    "probe-image-size": "^7.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-fast-marquee": "1.6.0",
    "react-lottie-player": "1.5.4",
    "react-toastify": "9.1.3",
    "rehype-document": "6.1.0",
    "rehype-external-links": "2.1.0",
    "rehype-format": "4.0.1",
    "rehype-highlight": "6.0.0",
    "rehype-raw": "^6.1.1",
    "rehype-sanitize": "5.0.1",
    "rehype-stringify": "9.0.3",
    "remark": "14.0.3",
    "remark-parse": "10.0.2",
    "remark-rehype": "10.1.0",
    "sharp": "^0.31.3",
    "swiper": "9.4.1",
    "unified": "10.1.2",
    "words-count": "2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@fairdataihub/config": "2.2.3",
    "@iconify/react": "4.1.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "10.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.4",
    "@semantic-release/npm": "10.0.5",
    "@semantic-release/release-notes-generator": "11.0.4",
    "@tailwindcss/line-clamp": "0.4.4",
    "@tailwindcss/typography": "0.5.9",
    "@types/node": "18.17.5",
    "@types/react": "18.0.31",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "autoprefixer": "10.4.15",
    "commitizen": "4.3.0",
    "cypress": "12.17.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.2",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "front-matter": "^4.0.2",
    "husky": "8.0.3",
    "lint-staged": "14.0.0",
    "megasanjay-devmoji": "^1.2.1",
    "next-sitemap": "4.2.2",
    "postcss": "8.4.28",
    "prettier": "3.0.2",
    "prettier-plugin-tailwindcss": "0.5.3",
    "semantic-release": "21.1.1",
    "tailwindcss": "3.3.3",
    "tailwindcss-debug-screens": "2.2.1",
    "typescript": "5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=18.0.0 "
  },
  "bugs": {
    "url": "https://github.com/fairdataihub/fairdataihub-website/issues/new/choose"
  },
  "homepage": "https://fairdataihub.org"
}
